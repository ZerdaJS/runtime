{
    "name": "xor",
    "version": "0.0.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "braintime-build ."
    },
    "dependencies": {
        "brain.js": "^2.0.0-beta.23"
    },
    "devDependencies": {
        "@braintime/build": "0.0.0"
    },
    "braintime": {
        "model": "@braintime/feed-forward",
        "layers": {
            "input": 2,
            "hidden": [ 3 ],
            "output": 1
        },
        "train": {
            "data": "training-data.json",
            "options": {
                "errorThresh": 0.011,
                "iterations": 1000
            }
        }
    }
}
